  "description": "Github User Search example",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "keywords": [
    "vue",
    "vuelm",
    "state management"
  ],
  "author": "Keuller Magalhaes",
